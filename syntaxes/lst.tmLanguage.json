{
    "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
    "name": "LST",
    "fileTypes": ["lst"],
    "patterns": [
        {
            "include": "#keywords"
        },
        {
            "include": "#strings"
        }
    ],
    "repository": {
        "keywords": {
            "patterns": [{
                "comment": "GAMS errors",
                "name": "support.function.lst",
                "match": "^\\*\\*\\*\\*.*$\n?"
            }]
        }
    },
    "scopeName": "source.lst"
}